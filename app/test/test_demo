def test_create_apartment(rooms, square, price, district):
     data = {
        'rooms': 1,
        'square': 25,
        'price': '4_444_000',
        'district': 'Советский'
     }

     result = {'rooms': 1, 'square': 25, 'price': '4_444_000', 'district': 'Советский'}
     assert data == result
